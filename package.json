    "configuration": {
      "type": "object",
      "properties": {
        "cursorPlus.reveal": {
          "type": "object",
          "properties": {
            "offset": {
              "type": "number",
              "default": 10,
              "description": "The minimum number of characters to reveal to the left."
            },
            "threshold": {
              "type": "number",
              "default": 20,
              "description": "If cursor position is less than this value, it will reveal from line start."
            }
          },
          "additionalProperties": false,
          "default": {},
          "description": "Reveal settings. Used when running a CursorPlus command that moves the cursor."
        },
        "cursorPlus.autoInsert": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": true,
              "description": "Enable/Disable auto-select behavior."
            },
            "escapeChar": {
              "type": "string",
              "default": "/",
              "description": "Start the text with this character to prevent any auto-select behavior."
            },
            "global": {
              "type": "array",
              "items": {
                "anyOf": [
                  {
                    "type": "string",
                    "description": "This text will be auto-inserted when typed."
                  },
                  {
                    "type": "object",
                    "properties": {
                      "text": {
                        "type": "string",
                        "description": "This text will be auto-inserted when its alias is typed."
                      },
                      "alias": {
                        "type": "string",
                        "description": "This alias will be typed to trigger the auto-insert."
                      }
                    },
                    "required": [
                      "text",
                      "alias"
                    ],
                    "additionalProperties": false
                  }
                ]
              },
              "default": [],
              "description": "Texts to auto-select for all insert commands."
            },
            "lineStart": {
              "$ref": "#/properties/cursorPlus.autoInsert/properties/global",
              "description": "Texts to auto-select for `cursorPlus.insert.lineStart` command."
            },
            "textStart": {
              "$ref": "#/properties/cursorPlus.autoInsert/properties/global",
              "description": "Texts to auto-select for `cursorPlus.insert.textStart` command."
            },
            "textEnd": {
