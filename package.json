          "default": {},
          "description": "Auto-select settings. Used when running a `cursorPlus.insert.*` command."
        },
        "cursorPlus.savedCommands": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "move",
              "select",
              "delete",
              "insert",
              "swap",
              "reorder",
              "runCommands"
            ]
          },
          "default": [
            "move",
            "select"
          ],
          "description": "Commands to save so that they can be re-run again using `cursorPlus.last` command.",
          "uniqueItems": true
        },
